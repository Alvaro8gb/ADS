class Instructors:
    def __init__(self,name,surname,phone):
        self.name= name
        self.surname = surname 
        self.phone = phone
  
class Students:
    def __init__(self, name,surname,phone,score):
        self.name = name
        self.surname = surname
        self.phone = phone
        self.score = 0
        self.instructor = None


